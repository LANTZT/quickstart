// -*- mode: javascript -*-
{
    "Name": "QsStokes",
    "Model":"Stokes",
    "Parameters":    {
        "Um": { "value":"1" },
        "H": { "value":"1" },
    },
    "Materials":
    {
        "fluid":
        {
            "name": "fluid",
            "rho": "1", // density
            "mu": "0.001" // kinematic_viscosity
        }
    },
    "BoundaryConditions":
    {
        "velocity":{
            "Dirichlet": {
                "inflow": { "expr":"{4*Um*y*( H-y )/H^2,0}:y:Um:H" },
                "wall": { "expr":"{0,0}" },
                "cylinder": { "expr":"{0,0}" }
            },
            "Neumann": {
                "outlet": { "expr":"{0,0,0,0}" }
            }
        }
    }, // BoundaryConditions
    "PostProcess":
    {
        "Fields":["pressure","velocity"],
    }
}

